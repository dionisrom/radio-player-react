{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
  { "handle": "filesystem" },
  { "src": "\/assets\/(.*)\\.webmanifest", "dest": "/assets/$1.webmanifest" },
  { "src": "/api/(.*)", "dest": "/api/$1.js" },
  { "src": "\/(.*)", "dest": "/index.html" }
  ]
}
